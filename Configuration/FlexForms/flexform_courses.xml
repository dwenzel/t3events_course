<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_tab.settings</sheetTitle>
					<cshFile>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_csh_flexform.xml</cshFile>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.mode</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.mode.course_list</numIndex>
										<numIndex index="1">Course->list;Course->show</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.mode.filter_courses</numIndex>
										<numIndex index="1">Course->filter</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.mode.lesson_list</numIndex>
										<numIndex index="1">Schedule->list;Schedule->show;Schedule->filter</numIndex>
									</numIndex>
									<numIndex index="3">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.mode.filter_lessons</numIndex>
										<numIndex index="1">Schedule->filter</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>

					<view.pluginNamespace>
						<TCEforms>
							<label>Plugin Namespace</label>
							<config>
								<type>input</type>
							</config>
						</TCEforms>
					</view.pluginNamespace>

					<!-- caching -->
					<settings.cache.makeNonCacheable>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.cache.makeNonCacheable</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.cache.makeNonCacheable>

					<settings.period>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period</label>
							<displayCond>FIELD:switchableControllerActions:!=:Course->filter</displayCond>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.all</numIndex>
										<numIndex index="1">all</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.futureOnly</numIndex>
										<numIndex index="1">futureOnly</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.pastOnly</numIndex>
										<numIndex index="1">pastOnly</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.specific</numIndex>
										<numIndex index="1">specific</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.period>

					<settings.periodType>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.constraintBy</label>
							<onChange>reload</onChange>
							<displayCond>FIELD:settings.period:=:specific</displayCond>
							<config>
								<type>radio</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.specific.byDay</numIndex>
										<numIndex index="1">byDay</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.specific.byMonth</numIndex>
										<numIndex index="1">byMonth</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.specific.byYear</numIndex>
										<numIndex index="1">byYear</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.period.specific.byDate</numIndex>
										<numIndex index="1">byDate</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.periodType>
					<settings.periodStart>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.start</label>
							<displayCond>FIELD:settings.periodType:!=:byDate</displayCond>
							<config>
								<type>input</type>
								<size>1</size>
								<range>
									<upper>999</upper>
									<lower>-999</lower>
								</range>
								<eval>int,trim</eval>
							</config>
						</TCEforms>
					</settings.periodStart>
					<settings.periodDuration>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.duration</label>
							<displayCond>FIELD:settings.periodType:!=:byDate</displayCond>
							<config>
								<size>3</size>
								<range>
									<upper>999</upper>
									<lower>1</lower>
								</range>
								<eval>int,trim</eval>
								<type>input</type>
							</config>
						</TCEforms>
					</settings.periodDuration>
					<settings.periodStartDate>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.start</label>
							<displayCond>FIELD:settings.periodType:=:byDate</displayCond>
							<config>
								<type>input</type>
								<size>8</size>
								<eval>date</eval>
								<checkbox>1</checkbox>
							</config>
						</TCEforms>
					</settings.periodStartDate>
					<settings.periodEndDate>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.end</label>
							<displayCond>FIELD:settings.periodType:=:byDate</displayCond>
							<config>
								<type>input</type>
								<size>8</size>
								<eval>date</eval>
								<checkbox>1</checkbox>
							</config>
						</TCEforms>
					</settings.periodEndDate>
					<settings.hideAfterDeadline>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.hideAfterDeadline</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.hideAfterDeadline>
					<settings.sortBy>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.sortBy</label>
							<displayCond>FIELD:switchableControllerActions:!=:Course->filter</displayCond>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.date</numIndex>
										<numIndex index="1">date</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.title</numIndex>
										<numIndex index="1">title</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.random</numIndex>
										<numIndex index="1">random</numIndex>
									</numIndex>

								</items>
							</config>
						</TCEforms>
					</settings.sortBy>

					<!-- sort direction -->
					<settings.sortDirection>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.sortDirection</label>
							<displayCond>FIELD:switchableControllerActions:!=:Course->filter</displayCond>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.ascending</numIndex>
										<numIndex index="1">asc</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.descending</numIndex>
										<numIndex index="1">desc</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.sortDirection>

					<settings.maxItems>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.maxItems</label>
							<displayCond>FIELD:switchableControllerActions:!=:Course->filter</displayCond>
							<config>
								<type>input</type>
								<size>3</size>
								<range>
									<upper>100000</upper>
									<lower>0</lower>
								</range>
								<eval>int,trim</eval>
							</config>
						</TCEforms>
					</settings.maxItems>

					<!--- Quick Menu Type -->
					<settings.filterType>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type</label>
							<displayCond>FIELD:switchableControllerActions:=:Course->filter</displayCond>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type.genres</numIndex>
										<numIndex index="1">genres</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type.venues</numIndex>
										<numIndex index="1">venues</numIndex>
									</numIndex>
									<numIndex index="3">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type.eventTypes</numIndex>
										<numIndex index="1">eventTypes</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.filterType>

				</el>
			</ROOT>
		</sDEF>
		<constraints>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_tab.constraints</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.constraintsConjunction>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.conjunction</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.conjunction.or</numIndex>
										<numIndex index="1">OR</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.conjunction.and</numIndex>
										<numIndex index="1">AND</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.constraintsConjunction>
					Conjunction>
					<!-- Genres -->
					<settings.genres>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type.genres</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_t3events_domain_model_genre</allowed>
								<size>5</size>
								<autoSizeMax>20</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>40</maxitems>
							</config>
						</TCEforms>
					</settings.genres>
					<!-- Course types -->
					<settings.eventTypes>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type.eventtypes</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_t3events_domain_model_eventtype</allowed>
								<size>5</size>
								<autoSizeMax>20</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>40</maxitems>
							</config>
						</TCEforms>
					</settings.eventTypes>
					<!-- EventLocations -->
					<settings.eventLocations>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type.eventlocations</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_t3events_domain_model_eventlocation</allowed>
								<size>5</size>
								<autoSizeMax>20</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>40</maxitems>
							</config>
						</TCEforms>
					</settings.eventLocations>
					<!-- EventLocations -->
					<settings.audiences>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.filter.type.audiences</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_t3events_domain_model_audience</allowed>
								<size>5</size>
								<autoSizeMax>20</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>40</maxitems>
							</config>
						</TCEforms>
					</settings.audiences>
				</el>
			</ROOT>
		</constraints>
		<!-- SHEET Template -->
		<template>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.template</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- Template Layout -->
					<settings.templateLayout>
						<TCEforms>
							<label>LLL:EXT:dakosy_reservations/Resources/Private/Language/locallang_be.xml:flexforms_general.templateLayout</label>
							<config>
								<type>select</type>
								<itemsProcFunc>CPSIT\T3eventsCourse\Hooks\ItemsProcFunc->user_templateLayout</itemsProcFunc>
								<items>
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.templateLayout>
				</el>
			</ROOT>
		</template>

	</sheets>
</T3DataStructure>
